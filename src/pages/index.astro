---
import HeroSection from "../components/hero-section.astro";
import BrandsSection from "../components/brands-section.astro";
import BenefitsSection from "../components/benefits-section.astro";
import WhoWeAreSection from "../components/who-we-are-section.astro";
import ServicesSection from "../components/services-section.astro";
import RequirementsSection from "../components/requirements-section.astro";
import Layout from "../layouts/Layout.astro";
---

<script>
  window.embeddedChatbotConfig = {
    chatbotId: "9eP8a_aLwx2B_KKmMkwDi",
    domain: "www.chatbase.co",
  };
</script>
<script
  src="https://www.chatbase.co/embed.min.js"
  chatbotId="9eP8a_aLwx2B_KKmMkwDi"
  domain="www.chatbase.co"
  defer></script>

<Layout title="Inverfin">
  <main class="relative">
    <HeroSection />
    <BenefitsSection />
    <ServicesSection />
    <BrandsSection />
    <WhoWeAreSection />
    <RequirementsSection />
  </main>
</Layout>
